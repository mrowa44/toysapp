= form_for(@toy) do |f|
  - if @toy.errors.any?
    #error_explanation
      %h2
        = pluralize(@toy.errors.count, "error")
        prohibited this toy from being saved:
      %ul
        - @toy.errors.full_messages.each do |message|
          %li= message
  .actions
    = collection_select :toy, :room_id, Room.all, :id, :name
    = f.submit
